prefix dcterms:       <http://purl.org/dc/terms/>
prefix foaf:          <http://xmlns.com/foaf/0.1/>
prefix org:           <http://www.w3.org/ns/org#>
prefix rdf:           <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: 	      <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd:           <http://www.w3.org/2001/XMLSchema#>
prefix shex:          <http://www.w3.org/2013/ShEx/ns#>
prefix dcat:         <http://www.w3.org/ns/dcat#>
prefix enn:           <http://www.w3.org/2002/07/owl#>
prefix ejprd:          <http://www.ejprarediseases.org/metadata#>

# Schema for Catalog
<catalog> {
  a                      ( dcat:Catalog)
, dcterms:identifier     xsd:string         #the url
, dcterms:alternateName  xsd:string         #the alternative name for the catalog
, ejprd:about            xsd:string         #information about the catalog
, foaf:name              xsd:string         #Name of Catalog
, dcterms:description    xsd:string         #Catalog description
, foaf:homepage          xsd:string         #the url
, enn:sameAs             xsd:string         #the url
, dcterms:location       xsd:string         #the organisation
, rdfs:label             xsd:string         #Pattern "Value of Catalog"@en

}
